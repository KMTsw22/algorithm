#include <iostream>
#define INF INT32_MAX
#define endl "\n"
using namespace std;
int graph[4][4] = {
        { 0, 5, INF, 8 },
        { 7, 0, 9, INF },
        { 2, INF, 0, 4 },
        { INF, INF, 3, 0 }
};
int main(){
    for (int i=0;i<4;i++){ //i는 거쳐가는 정점
        for (int k=0;i<4;i++){ //k는행
            for (int j=0;i<4;i++){ //j는 열 도착정점
                if (graph[i][k] + graph[k][j] < graph[k][j])
                    graph[k][j] = graph[i][k] + graph[k][j];
            }
        }
    }
    for (int i = 0;i < 4;i++){
        for (int j =0;j<4;j++){
            
            cout << graph[i] << " ";
        }
        cout << endl;
    }
}
